{
	"build": {
		"beforeDevCommand": "",
		"beforeBuildCommand": "",
		"devPath": "../src",
		"distDir": "../src",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "gridclock-scr",
		"version": "0.0.0"
	},
	"tauri": {
		"allowlist": {
			"window": {
				"setFullscreen": true
			},
			"shell": {
				"all": false,
				"open": true
			}
		},
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "dev.seanmamasde.gridclock-scr",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			]
		},
		"security": {
			"csp": null
		},
		"windows": [
			{
				"fullscreen": true,
				"resizable": true,
				"title": "gridclock-scr"
			}
		]
	}
}